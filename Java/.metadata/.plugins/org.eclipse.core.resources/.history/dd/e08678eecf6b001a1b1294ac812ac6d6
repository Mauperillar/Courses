
public class Matrix {

	public static void main(String[] args) {
		/*
		 * int[] array = new int[5]; array[0] = 0; array[1] = 1; array[2] = 2; array[3]
		 * = 3; array[4] = 4;
		 */

		int[] array = { 0, 1, 2, 3, 4 };

		for (int i = 0; i < array.length; i++) {
			System.out.println(array[i]);
		}
		
		for(int element: array) {
			System.out.println(array[element]);
		}

	}

}
