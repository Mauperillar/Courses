package graficos;

import javax.swing.*;

public class CreateFrames {
	public static void main(String[] args) {
		Ventana ventana = new Ventana();
	}
}


class Ventana extends JFrame{
	public Ventana() {
		setSize(500, 300);
		setVisible(true);
		this.setLocationRelativeTo(null);
		setDefaultCloseOperation(EXIT_ON_CLOSE);
	}
}
