
public class ManipulaCadenas {

	public static void main(String[] args) {
		String mi_nombre = "Mauricio";
		mi_nombre.length();
		mi_nombre.charAt(0); //Retorna la letra que está en cierta posición
		mi_nombre.substring(0,3); // Retorna las letras que está en cierto rango
		mi_nombre.equalsIgnoreCase("Mauricio");

	}

}
